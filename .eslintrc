{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-bitwise": 2,
    "no-console": 0,
    "no-multiple-empty-lines": 1,
    "no-use-before-define": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-case-declarations": 0
  }
}
